{
    "type": "object",
    "additionalProperties": true,
    "$schema": "http://json-schema.org/draft-03/schema",
    "required": true,
    "properties": {
        "type": {
            "type": "string",
            "required": true
        },
        "onFailure": {
            "type": "string",
            "required": false
        },
        "exception": {
            "type": "string",
            "required": true
        },
        "timestamp": {
            "type": "number",
            "required": true
        },
        "stack": {
            "type": "array",
            "required": true,
            "items": {
                "type": "object",
                "required": false,
                "additionalProperties": true,
                "properties": {
                    "class": {
                        "type": "string",
                        "required": true
                    },
                    "method": {
                        "type": "string",
                        "required": true
                    },
                    "line": {
                        "type": "integer",
                        "required": true
                    },
                    "signatures": {
                        "type": "array",
                        "required": true,
                        "items": {
                            "type": "string",
                            "required": false
                        }
                    }
                }
            }
        },
        "states": {
            "type": "array",
            "required": true,
            "items": [
                {
                    "type": "object",
                    "required": false,
                    "properties": {
                        "mods": {
                            "type": "array",
                            "required": false
                        },
                        "signature": {
                            "type": "string",
                            "required": false
                        }
                    }
                },
                {
                    "type": "object",
                    "required": false,
                    "properties": {
                        "mods": {
                            "type": "array",
                            "required": false,
                            "items": {
                                "type": "object",
                                "required": false,
                                "properties": {
                                    "modId": {
                                        "type": "string",
                                        "required": false
                                    },
                                    "state": {
                                        "type": "string",
                                        "required": false
                                    }
                                }
                            }
                        },
                        "signature": {
                            "type": "string",
                            "required": false
                        }
                    }
                }
            ]
        }
    }
}