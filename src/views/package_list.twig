{% extends 'base.twig' %}
{% block main %}
<div class="page-header">
    <h1>Packages</h1>
</div>
<p>There's too damn many. Temporarily disabled this while we think of a nicer way to display this data</p>
<ul>
    {% for package in packages %}
        <li><a href="{{ relative('package/' ~ package) }}">{{ package }}</a></li>
    {% endfor %}
</ul>

{% endblock %}